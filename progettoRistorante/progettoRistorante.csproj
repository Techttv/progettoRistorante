  <ItemGroup>
    <ProjectReference Include="..\CCStatusOrder\CCStatusOrder.csproj" />
    <ProjectReference Include="..\CCTavoli\CCTavoli.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="Font\merchant.ttf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
