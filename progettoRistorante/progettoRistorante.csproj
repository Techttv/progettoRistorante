    <Content Include="Font\Display\SF-Pro-Display-HeavyItalic.otf">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Include="Font\Display\SF-Pro-Display-Light.otf">
